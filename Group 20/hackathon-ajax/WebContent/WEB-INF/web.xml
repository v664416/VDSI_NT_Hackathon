<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>hackathon-web</display-name>
  
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
 
  <servlet>
    <servlet-name>searchServlet</servlet-name>
    <servlet-class>com.vplan.hackathon.SearchServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>searchServlet</servlet-name>
    <url-pattern>/searchServlet</url-pattern>
  </servlet-mapping>
  
   <servlet>
    <servlet-name>parallelSearchServlet</servlet-name>
    <servlet-class>com.vplan.hackathon.ParallelSearchServlet</servlet-class>
  </servlet>

  <servlet-mapping>
    <servlet-name>parallelSearchServlet</servlet-name>
    <url-pattern>/parallelSearchServlet</url-pattern>
  </servlet-mapping>
   
</web-app>